{
  "name": "pugchamp",
  "description": "PUG service",
  "private": true,
  "repository": "fwdcp/pugchamp",
  "dependencies": {
    "bluebird": "^3.5.2",
    "body-parser": "^1.18.3",
    "botkit": "^0.6.16",
    "chance": "^1.0.16",
    "co": "^4.6.0",
    "config": "^2.0.1",
    "connect-mongo": "^2.0.1",
    "debug": "^4.1.0",
    "discord.js": "^12.4.1",
    "distributions": "^1.1.0",
    "express": "^4.16.4",
    "express-session": "^1.12.1",
    "gamedig": "^1.0.46",
    "hbs": "^4.0.0",
    "heapdump": "^0.3.15",
    "http-status-codes": "^1.0.6",
    "humanize": "0.0.9",
    "idle-gc": "^1.0.1",
    "js-combinatorics": "^0.5.3",
    "jsonwebtoken": "^8.3.0",
    "limiter": "^1.1.3",
    "lodash": "^4.17.11",
    "mathjs": "^5.2.1",
    "modern-rcon": "^1.0.3",
    "moment": "^2.22.2",
    "moment-duration-format": "^2.2.2",
    "mongoose": "^5.3.4",
    "ms": "^2.1.1",
    "mz": "^2.4.0",
    "passport": "^0.4.0",
    "passport-openid": "github:fwdcp/passport-openid",
    "q": "^1.4.1",
    "redis": "^2.6.2",
    "request-promise": "^4.2.2",
    "serve-static": "^1.13.2",
    "socket.io": "^2.1.1",
    "socketio-jwt": "^4.4.0",
    "timesync": "^0.2.1",
    "twitter-text": "^1.13.4",
    "yargs": "^4.7.1"
  },
  "eslintConfig": {
    "env": {
      "es6": true,
      "node": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:lodash/recommended"
    ],
    "parserOptions": {
      "ecmaVersion": 8
    },
    "plugins": [
      "html",
      "lodash"
    ],
    "rules": {
      "array-callback-return": "error",
      "arrow-body-style": "error",
      "arrow-parens": [
        "error",
        "as-needed"
      ],
      "arrow-spacing": "error",
      "block-scoped-var": "error",
      "brace-style": [
        "error",
        "stroustrup"
      ],
      "callback-return": "error",
      "comma-style": "error",
      "curly": "error",
      "dot-location": "error",
      "dot-notation": "error",
      "eqeqeq": "error",
      "global-require": "error",
      "handle-callback-err": "error",
      "key-spacing": "error",
      "no-confusing-arrow": [
        "error",
        {
          "allowParens": true
        }
      ],
      "no-mixed-requires": "error",
      "no-new-require": "error",
      "no-path-concat": "error",
      "no-process-exit": "error",
      "no-shadow": "error",
      "no-throw-literal": "error",
      "no-unneeded-ternary": "error",
      "no-use-before-define": "error",
      "object-shorthand": "error",
      "operator-assignment": "error",
      "prefer-spread": "error",
      "prefer-template": "error",
      "require-await": "error",
      "quotes": [
        "error",
        "single"
      ],
      "template-curly-spacing": "error",
      "yoda": "error"
    }
  },
  "yargs": {
    "parse-numbers": false
  }
}
